class Employee:
    def __init__(self, name, salary, ssn):
        self.name = name
        self._salary = salary
        self._ssn = ssn

if __name__ =="__main__":
    emp = Employee("Asad" , 50000, "123-45-6789")
    print("Public Variable:", emp.name)
    print("Protected Variable:", emp._salary)
    print("Private Variable:", emp._ssn)
